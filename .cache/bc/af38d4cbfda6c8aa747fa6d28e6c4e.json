 \r\n        createToDo(todoList);\r\n        createMassiveBox();\r\n      \r\n        let boxesTodo = document.querySelector('.category-boxes-todo').querySelectorAll('.category-box');\r\n        for (let i = 0; i < boxesTodo.length; i++) {\r\n            boxesTodo[i].querySelector('.makeList').removeEventListener(\"mouseover\", makeTodoBigger, {once: true});\r\n            }\r\n         tmpTodoList = []; \r\n        }\r\n\r\n}\r\n\r\n//всё должно меняться с todoList"},"lineCount":null}},"error":null,"hash":"bb06500b059b9b34136a02b5b88b0421","cacheData":{"env":{}}}